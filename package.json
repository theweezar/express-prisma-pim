{
  "name": "express-prisma-pim",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "tsx src/index.ts",
    "watch": "tsx watch src/index.ts",
    "build:test": "tsc --resolveJsonModule",
    "babel": "babel ./dist/tsc --out-dir ./dist/babel && babel ./dist/tsc/prisma/json --out-dir ./dist/babel/prisma/json --copy-files",
    "test": "jest",
    "dump": "prisma migrate diff --from-empty --to-schema prisma/schema.prisma --script > prisma/migrations/0_init/migration.sql",
    "reset": "prisma migrate reset",
    "gen": "prisma generate",
    "seed": "tsx seed.ts"
  },
  "dependencies": {
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "express": "5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@tsconfig/node20": "^20.1.8",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.5",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "prisma": "^7.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
